{
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "cSpell.customDictionaries": {
    "custom-dictionary-workspace": {
      "name": "custom-dictionary-workspace",
      "path": "${workspaceFolder:career.necmos}/.cspell/custom-dictionary-workspace.txt",
      "addWords": true,
      "scope": "workspace"
    }
  },
  "cSpell.ignoreWords": [
    "AOSHIMA",
    "ASAKUSA",
    "Necmos",
    "Wantedly",
    "cacheclearparam",
    "eyecatch",
    "microcms",
    "notojp",
    "rightup",
    "webp"
  ]
}
